{
  "reporter": "stylint-json-reporter",
  "brackets": {
    "expect": "never",
    "error": true
  },
  "colons": {
    "expect": "never",
    "error": true
  },
  "colors": {
    "expect": "always",
    "error": true
  },
  "commaSpace": {
    "expect": "always",
    "error": true
  },
  "depthLimit": {
    "expect": 3,
    "error": true
  },
  "duplicates": {
    "expect": true,
    "error": true
  },
  "efficient": {
    "expect": "always",
    "error": true
  },
  "extendPref": {
    "expect": "@extends",
    "error": true
  },
  "indentPref": {
    "expect": 2,
    "error": true
  },
  "leadingZero": {
    "expect": "always",
    "error": true
  },
  "none": {
    "expect": "always",
    "error": true
  },
  "noImportant": {
    "expect": true,
    "error": true
  },
  "parenSpace": {
    "expect": "never",
    "error": true
  },
  "prefixVarsWithDollar": {
    "expect": "always",
    "error": true
  },
  "quotePref": {
    "expect": "single",
    "error": true
  },
  "semicolons": {
    "expect": "never",
    "error": true
  },
  "sortOrder": {
    "expect": "grouped"
  },
  "stackedProperties": {
    "expect": "never",
    "error": true
  },
  "trailingWhitespace": {
    "expect": "never",
    "error": true
  },
  "valid": {
    "expect": true,
    "error": true
  },
  "zeroUnits": {
    "expect": "never",
    "error": true
  },
  "zIndexNormalize": {
    "error": true
  }
}
