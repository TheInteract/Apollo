{
  "name": "Apollo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:ssr dev:hmr",
    "dev:ssr": "better-npm-run dev:ssr",
    "dev:hmr": "better-npm-run dev:hmr"
  },
  "betterScripts": {
    "dev:ssr": {
      "command": "nodemon ./ssr-server",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src"
      }
    },
    "dev:hmr": {
      "command": "node ./hmr-server",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "./src"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.prod.babel.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./src"
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-pure": "github:purely-functional/eslint-plugin-pure",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "import-sort-style-module": "^2.4.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "better-npm-run": "^0.0.14",
    "express": "^4.15.2",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    }
  }
}
